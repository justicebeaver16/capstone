{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
"scripts": {
  "dev": "nodemon app.js",
  "start": "node bin/www",
  "start:development": "nodemon bin/www",
  "start:production": "node bin/www",
  "build": "node psql-setup-script.js",

  "sequelize": "sequelize",
  "sequelize-cli": "sequelize-cli",

  "build:render": "npm run reset",
  "reset": "npm run migrate:undo && npm run migrate && npm run seed",
  "migrate:undo": "npx sequelize-cli db:migrate:undo:all --config config/database.js --seeders-path db/seeders",
  "migrate": "npx sequelize-cli db:migrate --config config/database.js --seeders-path db/seeders",
  "seed": "npx sequelize-cli db:seed:all --config config/database.js --seeders-path db/seeders"
},
  "engines": {
  "node": "18.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.1",
    "per-env": "^1.0.2",
    "pg": "^8.15.6",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.10",
    "sqlite3": "^5.1.7"
  },
  "description": ""
}
